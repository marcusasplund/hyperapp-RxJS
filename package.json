  },
  "homepage": "https://github.com/marcusasplund/hyperapp-RxJS#readme",
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },  
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-sw-cache": "^0.2.7",
    "standard": "*",
    "node-sass": "^4.12.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "build/"
